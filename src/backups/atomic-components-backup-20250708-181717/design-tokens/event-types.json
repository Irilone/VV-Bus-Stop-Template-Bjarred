      "accordionToggle": "vaccine:accordion:toggle",
      "priceToggle": "vaccine:price:toggle",
      "navigationToggle": "vaccine:nav:toggle",
      "tableSort": "vaccine:table:sort",
      "dateFormat": "vaccine:date:format"
    },
    "lifecycle": {
      "componentInit": "vaccine:component:init",
