{
  "eventTypes": {
    "user": {
      "click": "click",
      "hover": "mouseenter",
      "focus": "focus",
      "blur": "blur",
      "input": "input",
      "change": "change",
      "keydown": "keydown",
      "keyup": "keyup",
      "scroll": "scroll",
      "resize": "resize"
    },
    "custom": {
      "lightboxOpen": "vaccine:lightbox:open",
